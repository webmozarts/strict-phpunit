{
    "name": "webmozarts/strict-phpunit",
    "description": "Enables type-safe comparisons of objects in PHPUnit",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bernhard Schussek",
            "email": "bernhard.schussek@webmozarts.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "phpunit/phpunit": "^10.3 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.28",
        "infection/infection": "*"
    },
    "autoload": {
        "psr-4": {
            "Webmozarts\\StrictPHPUnit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmozarts\\StrictPHPUnit\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        },
        "gitlab-domains": [
            "gitlab.webmozarts.com"
        ],
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "require": "6.*.*"
        }
    }
}
